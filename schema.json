{
    "id":"spring-data-jpa",
    "properties":{
        "spring.datasource.url":"",
        "spring.datasource.username":"",
        "spring.datasource.password":"",
        "spring.main.web-application-type":""
    },
    "create-entity":true,
    "entities":[
        {
            "name":"User",
            "schema":{
                "id":{
                    "type":"String",
                    "primary":true
                },
                "username":{
                    "type":"String",
                    "require":true
                },
                "post":{
                    "type":"Post",
                    "relation":"OneToMany"
                }
            }
        },
        {
            "name":"Post",
            "schema":{
                "id":{
                    "type":"String",
                    "primary":true
                },
                "postName":{
                    "type":"String",
                    "require":true
                },
                "user":{
                    "type":"User",
                    "relation":"ManyToOne"
                }
            }
        }
    ]
}